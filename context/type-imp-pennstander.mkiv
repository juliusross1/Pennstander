     \definefontsynonym[PSBoldItalic]     			[file:Pennstander-ItalicBold.otf*textfeatures]        		[features=default]
     \definefontsynonym[PSExtraBoldItalic]     		[file:Pennstander-ItalicExtraBold.otf*textfeatures]        	[features=default]
     \definefontsynonym[PSBlackItalic]     			[file:Pennstander-ItalicBlack.otf*textfeatures]        		[features=default]

     % Math
     \definefontsynonym[PSMathThin]     			[file:PennstanderMath-Thin.otf*mathfeatureslight]       		 	
